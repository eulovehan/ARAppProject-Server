{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"outDir": "./dist",
		"esModuleInterop": true,
    	"experimentalDecorators": true,
    	"emitDecoratorMetadata": true,
		"strict": false,

		/* Advanced Options */
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}